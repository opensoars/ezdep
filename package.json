{
  "name": "ezdep",
  "version": "0.0.1",

  "description": "Node.js function dependencies (arguments) made easy",

  "scripts": {
    "test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha \"./test/mocha/*.js\" && npm run coveralls",
    "local_test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha \"./test/mocha/*.js\"",
    "coveralls": "node node_modules/coveralls/bin/coveralls.js < coverage/lcov.info",
    "doc": "node node_modules/jsdoc/jsdoc.js -c ./doc/config.json --readme README.md"
  },

  "engines": {
    "node": ">=0.10"
  },

  "dependencies": {
    
  },

  "devDependencies": {
    "mocha": ">=2.2.5",
    "istanbul": ">=0.3.17",
    "coveralls": ">=2.11.3",
    "jsdoc": ">=3.3.2"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/opensoars/ezdep"
  },

  "readmeFilename": "README.md",

  "license": "MIT",

  "author": "Sam [opensoars] <sam.opensoars@gmail.com> (https://opensoars.github.io/)",

  "keywords": [
    "dependencies",
    "dependency",
    "arguments",
    "argument",
    "parameters",
    "parameter",
    "automatization",
    "optimization",
    "consistency"
  ]
}